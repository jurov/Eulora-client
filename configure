    awk '
	BEGIN {
          FS="|";
          heading["yes"]="Found";
          heading["no"]="Missing";
          ok=""
        }
	!/^$/ {
	  if (ok != $3) {
            ok=$3;
            if (NR != 1)
              print "";
            print heading[ok]
          }
	  if ($3 != "no" || ($4 == "*" || $4 == "'"$cs_host_family"'"))
	  printf "  %-20s (%s)\n", $1, $2
	}'
    `
{ $as_echo "$as_me:${as_lineno-$LINENO}:
******************************************************************************
***                 Eulora  --   Configuration Summary                 ***
******************************************************************************
Build mode: ${build_mode}
Version: ${PACKAGE_VERSION} (SVN: ${ps_cv_source_is_svn})

Optional Dependencies
---------------------
$cs_summary" >&5
$as_echo "$as_me:
******************************************************************************
